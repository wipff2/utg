local Tab = Window:CreateTab("Main Tab", 4483362458) -- Title, Image


Tab:CreateButton({
    Name = "Copy Discord Link",
    Info = {
        Title = 'Copy Discord Link',
        Description = 'Copys our discord server link to your clipboard',
